{
    "discoveryServerAddr": "multi:///host1:port,host2:port,host3:port",
    "dc-id": "default",
    "database": "default",
    "appId": "local",
    "local.instanceGroups": [
        "local-group"
    ],
    "local.shards": [
        "shard1",
        "shard2"
    ],
    "shard1.listener_addr": "127.0.0.1:10000",
    "shard1.clusters": [
        "shard1-masters",
        "shard1-slaves"
    ],
    "shard1-masters.connect_timeout": "10s",
    "shard1-slaves.connect_timeout": "10s",
    "shard2.listener_addr": "127.0.0.1:10001",
    "shard2.clusters": [
        "shard2-masters",
        "shard2-slaves"
    ],
    "shard2-masters.connect_timeout": "10s",
    "shard2-slaves.connect_timeout": "10s"
}
